<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>genesis__Annual_Debt_Service__c</fullName>
    <deprecated>false</deprecated>
    <description>It stores annual debt service calculation for application.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL( genesis__Product_Type__c , &apos;LOAN&apos;), genesis__Payment_Amount__c*12, genesis__Credit_Limit__c* (genesis__Interest_Rate__c/100))</formula>
